function getRealFloor(americanFloor) {
  let europaFloor;
  if (americanFloor < 0 ) {
  europaFloor = americanFloor;
    } else if ( americanFloor == 0 || americanFloor == 1) {
      europaFloor = 0 ;
    }  else if ( americanFloor > 1 && americanFloor < 13) {
     europaFloor = americanFloor - 1 ;
    }  else if ( americanFloor == 13 ) {
      europaFloor = null ;
    } else {
       europaFloor = americanFloor - 2 ;
 }
      return europaFloor ;

 
}
